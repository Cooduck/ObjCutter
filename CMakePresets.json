{
    "version": 8,
    "configurePresets": [
        {
            "name": "ObjCutter",
            "displayName": "GCC 14.1.0 x86_64-w64-mingw32",
            "description": "使用编译器: C = D:/msys64/mingw64/bin/gcc.exe, CXX = D:/msys64/mingw64/bin/g++.exe",
            "generator": "MinGW Makefiles",
            "binaryDir": "${sourceDir}/out/build/${presetName}",
            "cacheVariables": {
                "CMAKE_INSTALL_PREFIX": "${sourceDir}/out/install/${presetName}",
                "CMAKE_C_COMPILER": "D:/msys64/mingw64/bin/gcc.exe",
                "CMAKE_CXX_COMPILER": "D:/msys64/mingw64/bin/g++.exe",
                "CMAKE_BUILD_TYPE": "Debug"
            }
        }
    ]
}